						<xs:annotation>
							<xs:documentation>Unidade de medida apropriada especificada em Lei Ordinaria para fins de apuração do Imposto Seletivo</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="TStringRTC">
								<xs:minLength value="1"/>
