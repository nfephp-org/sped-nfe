											<xs:restriction base="xs:string">
												<xs:whiteSpace value="preserve"/>
												<xs:pattern value="(0[1-9]|1[0-2])([/][2][0-9][0-9][0-9])"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="forDia" maxOccurs="31">
